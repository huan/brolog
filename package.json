{
  "name": "brolog",
  "version": "1.1.4",
  "description": "Npmlog like logger for Browser",
  "main": "bundles/brolog.js",
  "typings": "dist/brolog.d.ts",
  "scripts": {
    "clean": "rm -fr dist/* bundles/*",
    "dist": "npm run clean && npm run build && npm run build:umd",
    "build": "tsc",
    "build:umd": "tsc --target es5 --module umd --outDir bundles/",
    "build:amd:deprecated": "tsc --module amd --outDir dist/amd",
    "build:system:deprecated": "tsc --module system --outDir dist/system",
    "e2e": "test/protractor.sh",
    "lint": "npm run tslint",
    "tslint": "tslint src/**/*.ts && tsc --noEmit",
    "test": "npm run dist && npm run unit && npm run e2e",
    "unit": "tap -R tap test/unit/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zixia/brolog.git"
  },
  "keywords": [
    "brolog",
    "npmlog",
    "log",
    "logger",
    "console.log",
    "console",
    "browser"
  ],
  "author": "Zhuohuan LI <zixia@zixia.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zixia/brolog/issues"
  },
  "homepage": "https://github.com/zixia/brolog#readme",
  "devDependencies": {
    "jasmine-spec-reporter": "^4.0.0",
    "protractor": "^5.1.1",
    "sinon": "^2.1.0",
    "sinon-test": "^1.0.1",
    "tap": "^10.3.0",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typescript": "^2.3.2"
  },
  "files": [
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "bundles/",
    "dist/"
  ]
}
