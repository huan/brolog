  "devDependencies": {
    "jasmine-spec-reporter": "^4.0.0",
    "protractor": "^5.1.1",
    "sinon": "^2.1.0",
    "sinon-test": "^1.0.1",
    "tap": "^10.3.0",
    "tslint": "^5.1.0",
    "typescript": "^2.3.2"
  },
  "files": [
    "package.json",
