{
  "name": "brolog",
  "version": "1.8.3",
  "description": "Npmlog like logger for Browser",
  "main": "bundles/brolog.umd.js",
  "typings": "dist/src/brolog.d.ts",
  "scripts": {
    "clean": "rm -fr dist/* bundles/*",
    "dist": "npm run clean && npm run build && npm run rollup && npm run dist:es6to5",
    "dist:es6to5": "tsc --out ./bundles/brolog.umd.js --target es5 --allowJs bundles/brolog.es6.umd.js --lib es6,dom",
    "build": "tsc --module es6",
    "lint": "npm run lint:ts",
    "rollup": "rollup -c",
    "lint:ts": "tslint --project tsconfig.json && tsc --noEmit",
    "test": "npm run lint && npm run dist && npm run test:unit && npm run test:e2e",
    "test:e2e": "bash -x tests/protractor.sh",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "test:unit": "tap -R tap \"src/**/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huan/brolog.git"
  },
  "keywords": [
    "npmlog",
    "log",
    "logger",
    "console.log",
    "browser",
    "angular"
  ],
  "author": "Zhuohuan LI <zixia@zixia.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/huan/brolog/issues"
  },
  "homepage": "https://github.com/huan/brolog#readme",
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^10.0.3",
    "@types/puppeteer": "^1.3.0",
    "@types/rollup-plugin-json": "^3.0.0",
    "@types/semver": "^5.5.0",
    "@types/sinon": "^5.0.0",
    "@types/sinon-test": "^1.0.2",
    "chokidar": "^2.0.3",
    "git-scripts": "^0.2.1",
    "jasmine-spec-reporter": "^4.0.0",
    "magic-string": "^0.25.0",
    "protractor": "^5.3.1",
    "puppeteer": "^1.3.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup": "^0.65.2",
    "semver": "^5.5.0",
    "sinon": "^6.0.0",
    "sinon-test": "git://github.com/huan/sinon-test.git#patch-1",
    "source-map": "^0.7.2",
    "tap": "^12.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.7.0",
    "typescript": "^3.0.3"
  },
  "files": [
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "bundles/",
    "src/",
    "dist/"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "git": {
    "scripts": {
      "pre-push": "bash ./scripts/pre-push.sh"
    }
  },
  "dependencies": {}
}
